<table class="table">
  <h2>My time logs</h2>
  <th>Date</th>
  <th>Project</th>
  <th>Time Entered</th>
  <th></th>
  <tbody>
    <% @time_logs.each do |time_log| %>
    <%= content_tag_for :tr, time_log do %>
    <td>
      <%= time_log.date %>
    </td>
    <td>
      <!-- <%#= time_log.project.name if time_log.project %>  #synonymous with: -->
      <!-- <%#= time_log.project&.name %>                     #synonymous with: -->
      <%= time_log.project.try(:name)  %>
    </td>
    <td>
      <%= time_log.minutes %> minutes
    </td>
    <td>
      <!-- will delete this developer line later; using now to confirm only time entries for logged in person display -->
      < <%= time_log.developer.name %> >
      <%= link_to "Edit", edit_time_log_path(time_log.id) %>  |
      <%= link_to "Delete", time_log_path(time_log.id), method: :delete %>
    </td>
    <% end %>
    <% end %>
  </tbody>
</table>
